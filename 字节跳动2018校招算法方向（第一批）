def pref(p1,p2):
    if p1[0]>=p2[0] and p1[1]>=p2[1]:
        return True
    return False

n = int(input())
liste = []
for _ in range(n):
    p2 = list(map(int,input().split()))
    for p1 in liste:
        if pref(p1,p2):
            break
        liste.append(p2)

for p in liste:
    print(p[0],p[1])
